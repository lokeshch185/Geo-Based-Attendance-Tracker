{
  "expo": {
    "scheme": "acme",
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "name": "attendence-tracker",
    "slug": "attendence-tracker",
    "android": {
      "package": "com.anonymous.attendencetracker",
      "permissions": ["android.permission.CAMERA"]
    }
    
  }
}
